<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <router id="admin">

        <!-- Route ID should be equal to front name: https://github.com/magento/magento2/issues/7557 -->
        <route id="loginas" frontName="loginas">
            <module name="Flancer32_LoginAs"/>
        </route>

    </router>
</config>